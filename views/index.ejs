        </form>
    </div>
    <div id="imgcontainer">
        <div class="loader" id="loader"></div>
        <img src="<%= imageurl %>" width="500px" height="500px" id="image" alt="External Image">
    </div>
    <script>
        const heading = "Reddit image scraper"; // Your heading text
        const typingHeading = document.getElementById('typing-heading');

        function typeHeading(index) {
            typingHeading.innerHTML = heading.substring(0, index);
            if (index < heading.length) {
                setTimeout(function () {
                    typeHeading(index + 1);
                }, 100); // Adjust the timeout to control the typing speed
            }
        }

        // Start typing on page load
        if(!localStorage.getItem("visited")){
            localStorage.setItem('visited',true)
            typeHeading(0);
        }
        else{
            typingHeading.textContent = heading;
        }
        const but=document.getElementById('generate')
        const img=document.getElementById('image')
        const load=document.getElementById('loader')
        document.onreadystatechange=()=>{
            if(document.readyState !== 'complete'){
                img.style.display='none'
                load.style.display='block'
            }
            else{
                img.style.display='block';
                load.style.display='none';
            
            }
            
        }
        but.addEventListener('click',()=>{
            location.reload();
        })
    </script>
</body>
</html>
